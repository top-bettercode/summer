{
  "groups": [
    {
      "name": "logging",
      "type": "cn.bestwu.logging.logback.Logback2LoggingSystem"
    }
  ],
  "properties": [
    {
      "name": "app.project.name",
      "type": "java.lang.String",
      "description": "项目名称.",
      "sourceType": "cn.bestwu.logging.UtilKt"
    },
    {
      "name": "logging.log-url",
      "type": "java.lang.String",
      "description": "日志浏览前缀路径.",
      "sourceType": "cn.bestwu.logging.logback.Logback2LoggingSystem"
    },
    {
      "name": "logging.warn-subject",
      "type": "java.lang.String",
      "description": "日志报警标题.",
      "defaultValue": "${spring.application.name}  ${spring.profiles.active} system exception;",
      "sourceType": "cn.bestwu.logging.UtilKt"
    },
    {
      "name": "logging.spilt",
      "type": "java.util.Map<java.lang.String,java.lang.String>",
      "description": "Log levels severity mapping. Use 'root' for the root logger.Stored in different files, respectively.",
      "sourceType": "cn.bestwu.logging.logback.Logback2LoggingSystem"
    },
    {
      "name": "logging.spilt-marker",
      "type": "java.util.Map<java.lang.String,java.lang.String>",
      "description": "Log levels severity mapping.Delimited files based on markup.",
      "sourceType": "cn.bestwu.logging.logback.Logback2LoggingSystem"
    },
    {
      "name": "logging.spilt-level",
      "type": "java.lang.String[]",
      "description": "Log levels.Stored in different files, respectively.",
      "sourceType": "cn.bestwu.logging.logback.Logback2LoggingSystem"
    },
    {
      "name": "logging.show.enabled",
      "sourceType": "cn.bestwu.logging.RequestLoggingConfiguration",
      "description": "是否启用日志浏览功能.",
      "defaultValue": true,
      "type": "java.lang.Boolean"
    },
    {
      "name": "logging.view.path",
      "sourceType": "cn.bestwu.logging.LogsController",
      "defaultValue": "/logs",
      "description": "日志浏览路径.",
      "type": "java.lang.String"
    }
  ],
  "hints": [
    {
      "name": "logging.spilt.keys",
      "values": [
        {
          "value": "root",
          "description": "Root logger used to assign the default logging level."
        }
      ],
      "providers": [
        {
          "name": "logger-name"
        }
      ]
    },
    {
      "name": "logging.spilt.values",
      "values": [
        {
          "value": "trace"
        },
        {
          "value": "debug"
        },
        {
          "value": "info"
        },
        {
          "value": "warn"
        },
        {
          "value": "error"
        },
        {
          "value": "fatal"
        },
        {
          "value": "off"
        }
      ],
      "providers": [
        {
          "name": "any"
        }
      ]
    },
    {
      "name": "logging.spilt-level",
      "values": [
        {
          "value": "trace"
        },
        {
          "value": "debug"
        },
        {
          "value": "info"
        },
        {
          "value": "warn"
        },
        {
          "value": "error"
        },
        {
          "value": "fatal"
        },
        {
          "value": "off"
        }
      ],
      "providers": [
        {
          "name": "any"
        }
      ]
    },
    {
      "name": "logging.spilt-marker.keys",
      "providers": [
        {
          "name": "any"
        }
      ]
    },
    {
      "name": "logging.spilt-marker.values",
      "values": [
        {
          "value": "trace"
        },
        {
          "value": "debug"
        },
        {
          "value": "info"
        },
        {
          "value": "warn"
        },
        {
          "value": "error"
        },
        {
          "value": "fatal"
        },
        {
          "value": "off"
        }
      ],
      "providers": [
        {
          "name": "any"
        }
      ]
    }
  ]
}

